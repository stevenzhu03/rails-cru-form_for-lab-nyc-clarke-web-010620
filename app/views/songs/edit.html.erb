<h1>Edit Song</h1>

<%= form_for @song do |f| %>

<%= f.label :name, "Name" %>
<%= f.text_field :name %>


  <label>Artist id:</label><br>
  <%= f.text_field :artist_id %><br>

  <label>Genre id:</label><br>
  <%= f.text_field :genre_id %><br>

<%= f.submit "Update Song", disable_with: 'Editing Song...' %>

<% end %>